                        <death when="1691-10-22" cert="high"/>
                        <occupation>Imprimeur-libraire</occupation>
                        <residence cert="high">
                            <address>
                                <objectName/>
                                <street>rue des Limandes ; près de Saint-Maclou</street>
                                <settlement ref="geonames:2986140">Pontoise</settlement>
